{
  "name": "assetslink-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.development -- node --watch src/index.js",
    "start": "dotenv -e .env.production -- node src/index.js",
    "indexer": "node src/indexer/tokenLaunchIndexer.js",
    "indexer:sepolia": "dotenv -e .env.development -e .env.sepolia -- node src/indexer/tokenLaunchIndexer.js",
    "indexer:base-sepolia": "dotenv -e .env.development -e .env.basesepolia -- node src/indexer/tokenLaunchIndexer.js",
    "indexer:bsc-testnet": "dotenv -e .env.development -e .env.bsctest -- node src/indexer/tokenLaunchIndexer.js",
    "indexer:mainnet": "dotenv -e .env.production -e .env.mainnet -- node src/indexer/tokenLaunchIndexer.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mysql2": "^3.16.2",
    "sequelize": "^6.37.7",
    "dotenv": "^16.4.5",
    "viem": "2.21.55",
    "@goplus/sdk-node": "1.1.1",
    "dotenv-cli": "^7.4.2",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
